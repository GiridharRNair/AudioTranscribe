# .ebextensions/ffmpeg.config

packages:
  yum:
    autoconf: []
    automake: []
    cmake: []
    freetype-devel: []
    gcc: []
    gcc-c++: []
    git: []
    libtool: []
    make: []
    nasm: []
    pkgconfig: []
    zlib-devel: []
sources:
  /usr/local/src: http://ffmpeg.org/releases/ffmpeg-4.2.9.tar.bz2
commands:
  ffmpeg_install:
      cwd: /usr/local/src/ffmpeg-4.2.9
      command: sudo ./configure --prefix=/usr && make && make install